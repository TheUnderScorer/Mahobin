{
  "name": "mahobin",
  "version": "1.2.1",
  "description": "Yet another DI library for javascript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheUnderScorer/mahobin.git"
  },
  "license": "MIT",
  "author": {
    "name": "Przemysław Żydek",
    "email": "przemyslawzydek@gmail.com",
    "url": "https://github.com/TheUnderScorer"
  },
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "DI",
    "Dependency Injection",
    "IoC",
    "Inversion of Control",
    "Container",
    "Scoped Container",
    "TypeScript",
    "typesafe"
  ],
  "bugs": {
    "url": "https://github.com/TheUnderScorer/mahobin/issues"
  },
  "homepage": "https://github.com/TheUnderScorer/mahobin#readme",
  "dependencies": {
    "emittery": "0.10.2",
    "nanoid": "^3.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typedoc": "^0.22.14",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  }
}
